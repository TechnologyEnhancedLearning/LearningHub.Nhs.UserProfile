@model LearningHub.Nhs.LearningCredentials.Models.Dsp.UserVerifiableCredentialResponse

@{
    ViewData["Title"] = "Details";
}

<div class="nhsuk-width-container app-width-container nhsuk-u-padding-bottom-9">
    <h1>@Html.DisplayFor(model => model.CredentialName)</h1>

    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SerialNumber)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SerialNumber)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ActivityDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ActivityDate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.AttainmentStatus)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.AttainmentStatus)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.AddedToDSPDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.AddedToDSPDate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Status)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Status)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="ListUserCredentials" asp-route-id="@Model.VerifiableCredentialId">Back to List</a>
    </div>
</div>